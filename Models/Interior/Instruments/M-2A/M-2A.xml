<?xml version="1.0"?>

<PropertyList>

<path>M-2A.ac</path>

<animation>
 <type>material</type>
 <object-name>face</object-name>
 <object-name>M-2A.needle1</object-name>
 <object-name>M-2A.needle2</object-name>
 <emission>
  <red>0.8</red>
  <green>0.8</green>
  <blue>0.8</blue>
  
 </emission>
</animation>

<animation>
 <type>material</type>
 <object-name>screws</object-name>
