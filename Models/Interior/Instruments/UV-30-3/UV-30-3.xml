<?xml version="1.0"?>

<PropertyList>

<path>UV-30-3.ac</path>

<animation>
 <type>material</type>
 <object-name>UV-30-3.needle1</object-name>
 <object-name>UV-30-3.needle2</object-name>
 <object-name>UV-30-3.DC1</object-name>
 <object-name>UV-30-3.DC2</object-name>
 <object-name>UV-30-3.DC3</object-name>
 <object-name>UV-30-3.Cm</object-name>
 <object-name>UV-30-3.PD1</object-name>
 <object-name>UV-30-3.PD2</object-name>
 <object-name>face</object-name>
 <emission>
  <red>0.8</red>
  <green>0.8</green>
  <blue>0.8</blue>
  
 </emission>
</animation>

<animation>
 <type>material</type>
 <object-name>screws</object-name>
 <emission>
  <red>0.8</red>
  <green>0.8</green>
  <blue>0.8</blue>
  
 </emission>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UV-30-3.needle1</object-name>
 <property>mig29/instrumentation/UV-30-3/indicated-altitude-m</property>
 <interpolation>
  <entry><ind>-1000</ind><dep> -360</dep></entry>
  <entry><ind>    0</ind><dep>    0</dep></entry>
  <entry><ind>30000</ind><dep>10800</dep></entry>
 </interpolation>
 <axis>
  <x>-1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UV-30-3.needle2</object-name>
 <property>mig29/instrumentation/UV-30-3/indicated-altitude-m</property>
 <interpolation>
  <entry><ind>-7500</ind><dep>-90</dep></entry>
  <entry><ind>    0</ind><dep>  0</dep></entry>
  <entry><ind>30000</ind><dep>360</dep></entry>
 </interpolation>
 <axis>
  <x>-1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UV-30-3.DC1</object-name>
 <property>mig29/instrumentation/UV-30-3/setting-mmhg-1</property>
 <factor>36</factor>
 <center>
  <x-m>-0.014</x-m>
  <y-m>-0.006</y-m>
  <z-m>-0.030</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UV-30-3.DC2</object-name>
 <property>mig29/instrumentation/UV-30-3/setting-mmhg-2</property>
 <factor>36</factor>
 <center>
  <x-m>-0.014</x-m>
  <y-m>0.000</y-m>
  <z-m>-0.030</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UV-30-3.DC3</object-name>
 <property>mig29/instrumentation/UV-30-3/setting-mmhg-3a</property>
 <factor>36</factor>
 <center>
  <x-m>-0.014</x-m>
  <y-m>0.006</y-m>
  <z-m>-0.030</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>UV-30-3.Cm</object-name>
 <property>mig29/instrumentation/UV-30-3/setting-mmhg-3a</property>
 <factor>3.6</factor>
 <center>
  <x-m>-0.014</x-m>
  <y-m>0.007</y-m>
  <z-m>-0.030</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>UV-30-3.PD1</object-name>
 <property>mig29/instrumentation/UV-30-3/indicated-altitude-m</property>
 <factor>0.00001</factor>
 <step>10000.0</step>
 <axis>
  <x>0</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>
<animation>
 <type>textranslate</type>
 <object-name>UV-30-3.PD2</object-name>
 <property>mig29/instrumentation/UV-30-3/indicated-altitude-m</property>
 <factor>0.0001</factor>
 <step>1000.0</step>
 <axis>
  <x>0</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>pick</type>
 <object-name>UV-30-3.cz1</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>true</repeatable>
  <binding>
   <command>property-adjust</command>
   <property>/instrumentation/altimeter/setting-inhg</property>
   <step>-0.00984252</step>
   <min>27.9</min>
   <max>31.4</max>
  </binding>
 </action>
</animation>

<animation>
 <type>pick</type>
 <object-name>UV-30-3.cz2</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>true</repeatable>
  <binding>
   <command>property-adjust</command>
   <property>/instrumentation/altimeter/setting-inhg</property>
   <step>0.00984252</step>
   <min>27.9</min>
   <max>31.4</max>
  </binding>
 </action>
</animation>

</PropertyList>