<?xml version="1.0"?>
 
<PropertyList>

 <fx>
 <!-- Internal Sounds -->
 
  <voice>
  <name>Geforce-breath-vox</name>
  <path>Aircraft/SU-27/Sounds/Geforce.wav</path>
  <condition>
   <greater-than>
    <property>/accelerations/pilot/z-accel-fps_sec</property>
    <value>-161</value>
   </greater-than>   
  </condition>
<volume>
<factor>0.9</factor>
</volume>
 </voice>

<voice>
  <name>Geforce-vox</name>
  <path>Aircraft/SU-27/Sounds/geforce1.wav</path>
  <condition>
   <less-than>
    <property>/accelerations/pilot/z-accel-fps_sec</property>
    <value>-225</value>
   </less-than>   
  </condition>
<volume>
<factor>0.9</factor>
</volume>
 </voice>
 
 <engine>
