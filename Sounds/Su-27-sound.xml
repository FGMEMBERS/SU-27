		   <value>1</value>
		  </equals>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume>
        <property>engines/engine[0]/rpm</property>
        <factor> 0.01 </factor>
        <min> 0.03 </min>
        <max> 0.07 </max>
      </volume>
      <pitch>
        <property>engines/engine[0]/rpm</property>
        <factor> 0.01 </factor>
        <min> 0.0 </min>
        <max> 0.9 </max>
      </pitch>
       <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
   </engine>
   
    <engine>
      <name>engine-start-inside1</name>
      <mode>once</mode>
